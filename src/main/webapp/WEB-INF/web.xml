<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>societates</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>Monitor</filter-name>
    <filter-class>dev.castanhocorreia.societates.controller.MonitorFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Monitor</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>Authenticate</filter-name>
    <filter-class>dev.castanhocorreia.societates.controller.AuthenticityFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Authenticate</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
</web-app>
